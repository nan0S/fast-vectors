#!/bin/sh

COMMON_OPTS="
    --verbose=3
"

if [ "$1" = "static" ]; then
    arg="static"
    shift
fi

if [ "$arg" = "static" ]; then
    ./run-push-only static $COMMON_OPTS $@
    ./run-push-cons-dest static $COMMON_OPTS $@
    ./run-all static $COMMON_OPTS $@
else
    # ./run-push-only-1 $COMMON_OPTS $@
    # ./run-push-only-3 $COMMON_OPTS $@
    # ./run-push-only-10 $COMMON_OPTS $@
    ./run-push-only-50 $COMMON_OPTS $@
    ./run-push-only-100 $COMMON_OPTS $@
    # ./run-push-cons-dest $COMMON_OPTS $@
    # ./run-all $COMMON_OPTS $@
fi
